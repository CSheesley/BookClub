<div id = <%="review-#{review.id}"%>>
  <ul>
    <li>
      <div id = title>
        <%=review.title %>
      </div>
    </li>
    <li>
      <div id = text>
        <%= review.text %>
      </div>
    </li>
    <li>
      <div id = rating>
        <%= review.rating %>
      </div>
    </li>
    <li>
      <div id = user>
        <a href = <%=reviews_path(user: review.user)%> >
          <%= review.user %>
        </a>
      </div>
    </li>

  </ul>
</div>
