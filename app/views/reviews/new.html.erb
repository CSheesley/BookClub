<h1>Add New Review</h1>

<%= form_for [@book, @review] do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>
<p>
  <%= f.label :user %>
  <%= f.text_field :user %>
</p>
<p>
  <%= f.label :rating %>
  <%= f.select :rating, options_for_select(1..5)%>
</p>
<p>
  <%= f.label :text %>
  <%= f.text_area :text %>
</p>
<p>
  <%= f.submit %>
</p>
<% end %>
