<div id = <%="book-card-#{book.id}"%> >
  <div id = book-title ><%= book.title %></div>
    <div id = book-pages>Pages: <%= book.pages %></div>
    <div id = book-year>Year of Publication: <%= book.year %></div>
    <div id = book-cover><img src = <%=book.cover%>></div>
    <div id = author-info>
      By:
      <%= render book.authors %>
    </div>
    <%= link_to 'New Review', new_book_review_path(book) %>
</div>
